@article{greljo_high-p_tdilepton_2017,
	title = {High-\$\$p\_T\$\$dilepton tails and flavor physics},
	volume = {77},
	issn = {1434-6052},
	url = {https://doi.org/10.1140/epjc/s10052-017-5119-8},
	doi = {10.1140/epjc/s10052-017-5119-8},
	abstract = {We investigate the impact of flavor-conserving, non-universal quark-lepton contact interactions on the dilepton invariant mass distribution in \$\$p{\textasciitilde}p {\textbackslash}rightarrow {\textbackslash}ell {\textasciicircum}+ {\textbackslash}ell {\textasciicircum}-\$\$processes at the LHC. After recasting the recent ATLAS search performed at 13 TeV with 36.1 fb\$\${\textasciicircum}\{-1\}\$\$of data, we derive the best up-to-date limits on the full set of 36 chirality-conserving four-fermion operators contributing to the processes and estimate the sensitivity achievable at the HL-LHC. We discuss how these high-\$\$p\_T\$\$measurements can provide complementary information to the low-\$\$p\_T\$\$rare meson decays. In particular, we find that the recent hints on lepton-flavor universality violation in \$\$b {\textbackslash}rightarrow s {\textbackslash}mu {\textasciicircum}+ {\textbackslash}mu {\textasciicircum}-\$\$transitions are already in mild tension with the dimuon spectrum at high-\$\$p\_T\$\$if the flavor structure follows minimal flavor violation. Even if the mass scale of new physics is well beyond the kinematical reach for on-shell production, the signal in the high-\$\$p\_T\$\$dilepton tail might still be observed, a fact that has been often overlooked in the present literature. In scenarios where new physics couples predominantly to third generation quarks, instead, the HL-LHC phase is necessary in order to provide valuable information.},
	language = {en},
	number = {8},
	urldate = {2022-03-02},
	journal = {The European Physical Journal C},
	author = {Greljo, Admir and Marzocca, David},
	month = aug,
	year = {2017},
	pages = {548},
}

